Algoritmo "maior_numero_informado"

Var

   n1, n2, n_maior : real

Inicio

   escreva("Digite um numero real: ")
   leia(n1)
   escreva("Digite outro numero real: ")
   leia(n2)
   
   se n1 > n2 entao
     escreval("O maior numero digitado foi: ", n1)
   senao
     escreval("O maior numero digitado foi: ", n2)
   fimse
   
   
Fimalgoritmo